<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BankServer.BankMain">
   <children>
      <ComboBox fx:id="cmbBank" layoutX="25.0" layoutY="63.0" onMouseClicked="#setBanksInComboBox" prefWidth="150.0" style="-fx-background-radius: 5;" />
      <Button layoutX="25.0" layoutY="111.0" mnemonicParsing="false" onAction="#chooseBankToStartUpFrom" prefHeight="25.0" prefWidth="150.0" style="-fx-background-radius: 5;" text="Choose">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Button>
      <Label layoutX="25.0" layoutY="14.0" prefHeight="45.0" prefWidth="150.0" text="Which bank do you want to start up?" wrapText="true">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
